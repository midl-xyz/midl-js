{
	"$schema": "https://turbo.build/schema.json",
	"extends": ["//"],
	"tasks": {
		"test:compile": {
			"dependsOn": ["build"],
			"outputs": [],
			"cache": false
		},
		"test": {
			"dependsOn": ["build", "^build", "test:compile"],
