{
	"name": "@midl/playwright",
	"version": "3.0.2-next.1",
	"license": "MIT",
	"scripts": {
		"build": "tsdown --format cjs,esm -d dist",
		"test": "vitest",
		"postinstall": "tsx ./scripts/prepare.ts"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.cts",
	"files": [
		"dist",
		"scripts"
	],
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./*": {
			"import": {
				"types": "./dist/*.d.mts",
				"default": "./dist/*.mjs"
			},
			"require": {
				"types": "./dist/*.d.cts",
				"default": "./dist/*.cjs"
			}
		}
	},
	"devDependencies": {
		"@playwright/test": "^1.48.2",
		"@types/unzipper": "^0.10.10"
	},
	"peerDependencies": {
		"@playwright/test": ">=1.48 <2"
	},
	"dependencies": {
		"@midl/core": "workspace:^",
		"rxjs": "^7.8.2",
		"tsx": "^4.19.2",
		"unzipper": "^0.12.3"
	},
	"repository": {
		"url": "git+https://github.com/midl-xyz/midl-js.git",
		"directory": "packages/playwright",
		"type": "git"
	}
}
