{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"cleanUrls": true,
	"build": {
		"env": {
			"ENABLE_EXPERIMENTAL_COREPACK": "1"
		}
	},
	"redirects": [
		{
			"source": "/intro/overview",
			"destination": "/docs",
			"statusCode": 301
		},
		{
			"source": "/bitcoin/react/overview",
			"destination": "/hooks/overview",
			"statusCode": 301
		}
	]
}
