{
	"$schema": "https://turbo.build/schema.json",
	"ui": "stream",
	"globalPassThroughEnv": ["COREPACK_HOME"],
	"tasks": {
		"dev": {
			"dependsOn": ["^build"],
			"persistent": true
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist"]
		},
		"test": {
			"dependsOn": ["^build"],
			"cache": false,
			"outputs": []
		}
	}
}
